@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix mo: <http://purl.org/ontology/mo/> .
@prefix mt: <http://purl.org/ontology/studio/multitrack#> .
@prefix ch: <http://tiny.cc/charm-ontology#> .
@prefix dy: <http://tiny.cc/dymo-ontology#> .


dy:
	a owl:Ontology ;
	rdfs:comment "An ontology for describing Dynamic Music Objects" .

dy:Dymo
	a owl:Class ;
	rdfs:subClassOf ch:Constituent ;
	rdfs:comment "A Dynamic Music Object is a hierarchical structure of musical objects with modifiable parameters" .

dy:Feature
	a owl:Class ;
	rdfs:subClassOf ch:Attribute ;
	rdfs:comment "A parameter is an immutable attribute of a Dymo" .

dy:Parameter
	a owl:Class ;
	rdfs:subClassOf ch:Attribute ;
	rdfs:comment "A parameter is a mutable attribute of a Dymo" .

dy:ParameterPreset
	a owl:Class ;
	rdfs:comment "A parameter preset" .

#Audio parameters

dy:AudioParameter
	a owl:Class ;
	rdfs:subClassOf dy:Parameter ;
	rdfs:comment "" .

dy:Amplitude
	a dy:AudioParameter ;
	rdfs:comment "" .

dy:PlaybackRate
	a dy:AudioParameter ;
	rdfs:comment "" .

dy:Pan
	a dy:AudioParameter ;
	rdfs:comment "" .

dy:Distance
	a dy:AudioParameter ;
	rdfs:comment "" .

dy:Reverb
	a dy:AudioParameter ;
	rdfs:comment "" .

#Structural parameters

dy:StructuralParameter
	a owl:Class ;
	rdfs:subClassOf dy:Parameter ;
	rdfs:comment "" .

dy:PartOrder
	a dy:StructuralParameter ;
	rdfs:comment "" .

dy:PartCount
	a dy:StructuralParameter ;
	rdfs:comment "" .

dy:PartDurationRatio
	a dy:StructuralParameter ;
	rdfs:comment "" .

dy:PartProportion
	a dy:StructuralParameter ;
	rdfs:comment "" .


#PROPERTIES

#Dymo properties

dy:hasSource
	a owl:DatatypeProperty ;
	rdfs:domain dy:Dymo ;
	rdfs:range xsd:string ;
	rdfs:comment "" .

dy:hasPreset
	a owl:ObjectProperty ;
	rdfs:domain dy:Dymo ;
	rdfs:range dy:Parameter ;
	rdfs:comment "" .

#Preset properties

dy:hasParameter
	a owl:ObjectProperty ;
	rdfs:domain dy:ParameterPreset ;
	rdfs:range dy:Parameter ;
	rdfs:comment "" .

dy:hasValue
	a owl:DatatypeProperty ;
	rdfs:domain dy:ParameterPreset ;
	rdfs:range xsd:float ;
	rdfs:comment "" .