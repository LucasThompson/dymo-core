#@base <http://purl.org/ontology/studio/mobile#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix mo: <http://purl.org/ontology/mo/> .
@prefix mt: <http://purl.org/ontology/studio/multitrack#> .
@prefix ch: <http://???/co/charm#> .
@prefix : <#> .


<#>
	a owl:Ontology ;
	rdfs:comment "An ontology for describing Dynamic Music Objects" .

:Dymo
	a owl:Class ;
	rdfs:subClassOf ch:Constituent ;
	rdfs:comment "A Dynamic Music Object is a hierarchical structure of musical objects with modifiable parameters" .

:Feature
	a owl:Class ;
	rdfs:subClassOf ch:Attribute ;
	rdfs:comment "A parameter is an immutable attribute of a Dymo" .

:Parameter
	a owl:Class ;
	rdfs:subClassOf ch:Attribute ;
	rdfs:comment "A parameter is a mutable attribute of a Dymo" .

:ParameterPreset
	a owl:Class ;
	rdfs:comment "A parameter preset" .

#Audio parameters

:AudioParameter
	a owl:Class ;
	rdfs:subClassOf :Parameter ;
	rdfs:comment "" .

:Amplitude
	a :AudioParameter ;
	rdfs:comment "" .

:PlaybackRate
	a :AudioParameter ;
	rdfs:comment "" .

:Pan
	a :AudioParameter ;
	rdfs:comment "" .

:Distance
	a :AudioParameter ;
	rdfs:comment "" .

:Reverb
	a :AudioParameter ;
	rdfs:comment "" .

#Structural parameters

:StructuralParameter
	a owl:Class ;
	rdfs:subClassOf :Parameter ;
	rdfs:comment "" .

:PartOrder
	a :StructuralParameter ;
	rdfs:comment "" .

:PartCount
	a :StructuralParameter ;
	rdfs:comment "" .

:PartDurationRatio
	a :StructuralParameter ;
	rdfs:comment "" .

:PartProportion
	a :StructuralParameter ;
	rdfs:comment "" .


#PROPERTIES

#Dymo properties

:hasSource
	a owl:DatatypeProperty ;
	rdfs:domain :Dymo ;
	rdfs:range xsd:string ;
	rdfs:comment "" .

:hasPreset
	a owl:ObjectProperty ;
	rdfs:domain :Dymo ;
	rdfs:range :Parameter ;
	rdfs:comment "" .

#Preset properties

:hasParameter
	a owl:ObjectProperty ;
	rdfs:domain :ParameterPreset ;
	rdfs:range :Parameter ;
	rdfs:comment "" .

:hasValue
	a owl:DatatypeProperty ;
	rdfs:domain :ParameterPreset ;
	rdfs:range xsd:float ;
	rdfs:comment "" .