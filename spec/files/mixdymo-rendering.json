{
	"@context":"http://localhost:8181/ontologies/dymo-context.json",
	"@id":"demoBeatRendering",
	"@type":"Rendering",
	"dymo":"demoBeat",
	"mappings":[
		{
			"function":{"args":[
				{"var":"a", "val":{
					"@type":"DataControl",
					"url":"http://api.openweathermap.org/data/2.5/weather?appid=3d77879a046ee9e970e66bb2f5c5200d&q=london",
					"map":"return json['main']['temp']"}
				}
			],"body": "return (a-273.16)/40;"},
			"targets":["mixdymo"],
			"range":"Fade"
		},
		{
			"function":{"args":[
				{"var":"a", "val":{"@id":"fade","name":"fade","@type":"Slider"}}
			],"body": "return a;"},
			"targets":["mixdymo"],
			"range":"Fade"
		},
		{
			"function":{"args":[
				{"var":"a", "val":{"name":"beats per bar","@type":"Slider"}}
			],"body": "return 4*a;"},
			"targets":{"args":[{"var":"d", "val":"level"}], "body": "return d == 2;"},
			"range":"PartCount"
		},
		{
			"function":{"args":[
				{"var":"a", "val":{"name":"offbeat duration","@type":"Slider"}}
			],"body": "return a;"},
			"targets":{
				"args":[{"var":"l", "val":"level"},{"var":"i", "val":"index"}],
				"body": "return l == 3 && i%2 == 1;"
			},
			"range":"DurationRatio"
		},
		{
			"function":{"args":[
				{"var":"a", "val":{"@id":"transition","name":"transition","@type":"Button"}}
			],"body": "return a;"},
			"targets":["fadeRamp"],
			"range":"AutoControlTrigger"
		}
	]
}
