{
	"@context":"http://tiny.cc/dymo-context",
	"@id":"mixdymo",
	"@type":"Dymo",
	"cdt":"parallel",
	"mappings":[
		{
			"domainDims":[{"@id":"Fade","@type":"Parameter"},{"@id":"index","@type":"Feature"}],
			"function":{"args":["a", "b"],"body":"return (1-b)*(1-a)+b*a;"},
			"dymos":{"args":["d"],"body":"return d.getLevel() == 1;"},
			"range":"Amplitude"
		},
		{
			"domainDims":[{"@id":"Tempo","@type":"Parameter"},{"@id":"duration","@type":"Feature"}],
			"function":{"args":["a", "b"],"body":"return a/60*b;"},
			"dymos":{"args":["d"],"body":"return d.getLevel() == 3;"},
			"range":"TimeStretchRatio"
		}
	],
	"parts":["dymo2.json","dymo4.json"],
	"similars":[]
}