{
	"@context":"http://localhost:8181/ontologies/dymo-context.json",
	"@id":"mixdymo",
	"@type":"Dymo",
	"cdt":"Conjunction",
	"features":["index","duration"],
	"mappings":[
		{
			"@type":"Mapping",
			"function":{"args":[
				{"var":"a", "val":{"@id":"Fade","@type":"CustomParameter"}},
				{"var":"b", "val":"index"}
			],"body": "return (1-b)*(1-a)+b*a;"},
			"targets": {"args":[{"var":"a", "val":"level"}], "body":"return a == 1;"},
			"range":"Amplitude"
		},
		{
			"@type":"Mapping",
			"function":{"args":[
				{"var":"a", "val":{"@id":"Tempo","@type":"CustomParameter"}},
				{"var":"b", "val":"duration"}
			], "body": "return a/60*b;"},
			"targets": {"args":[{"var":"a", "val":"level"}], "body": "return a == 3;"},
			"range":"TimeStretchRatio"
		}
	],
	"parts":{"@list":["dymo2.json","dymo4.json"]},
	"similars":[]
}
