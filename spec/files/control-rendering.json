{
	"@context": "http://tiny.cc/dymo-context",
	"@id": "controlRendering",
	"@type": "Rendering",
	"dymo": "dymo0",
	"mappings": [{
		"domainDims": [{
			"name": "slidero",
			"@type": "Slider"
		}],
		"function": {
			"args": ["a"],
			"body": "return a;"
		},
		"targets": {
			"args": ["d"],
			"body": "return d.getLevel() == 1;"
		},
		"range": "Amplitude"
	}, {
		"domainDims": [{
			"name": "slidero",
			"@type": "Slider"
		}],
		"function": {
			"args": ["a"],
			"body": "return a;"
		},
		"targets": {
			"args": ["d"],
			"body": "return d.getLevel() == 2;"
		},
		"range": "PlaybackRate"
	}, {
		"domainDims": [{
			"@id":"orientation",
			"name": "orientation",
			"@type": "Slider"
		}],
		"function": {
			"args": ["a"],
			"body": "return 360*a;"
		},
		"range": "ListenerOrientation"
	}],
	"navigator": {
		"targets": {
			"args": ["d"],
			"body": "return d.getLevel() == 2;"
		},
		"@type": "SimilarityNavigator"
	}
}
