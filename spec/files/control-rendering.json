{
	"@context":"http://tiny.cc/dymo-context/",
	"@id": "controlRendering",
	"@type": "Rendering",
	"dymo": "dymo0",
	"mappings": [{
		"function": {
			"args": [{"var":"a", "val":{"name":"slidero","@type":"Slider"}}],
			"body": "return a;"
		},
		"targets": {
			"args": [{"var":"a", "val":"level"}],
			"body": "return a == 1;"
		},
		"range": "Amplitude"
	}, {
		"function": {
			"args": [{"var":"a", "val":{"name":"slidero","@type":"Slider"}}],
			"body": "return a;"
		},
		"targets": {
			"args": [{"var":"a", "val":"onset"}],
			"body": "return a > 0.7;"
		},
		"range": "PlaybackRate"
	}, {
		"function": {
			"args": [{
				"var":"a",
				"val":{
					"@id":"orientation",
					"name": "orientation",
					"@type": "Slider"
				}
			}],
			"body": "return 360*a;"
		},
		"range": "ListenerOrientation"
	}],
	"navigator": {
		"targets": {
			"args": [{"var":"a", "val":"level"}],
			"body": "return a == 2;"
		},
		"@type": "SimilarityNavigator"
	}
}
